{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "main": "app.js",
  "scripts": {
    {{#ts}}
    "start": "ts-node app.ts",
    "dev": "nodemon app.ts",
    {{/ts}}
    {{#if_eq ts false}}
    "start": "node app.js",
    "dev": "nodemon app.js",
    {{/if_eq}}
    {{#ts}}
    "build": "tsc --project ./",
    {{/ts}}
    "lint": "eslint --fix",
    "test": "mocha --exit"
  },
  "author": "{{ author }}",
  "license": "MIT",
  "dependencies": {
    {{#if_xor cors extraction "extraction"}}
    "cors": "^2.8.5",
    {{/if_xor}}
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "debug": "^4.3.1",
    {{#helmet}}
    "helmet": "^4.4.1",
    {{/helmet}}
    {{#morgan}}
    "morgan": "^1.10.0",
    {{/morgan}}
    {{#if_eq database "mongoose"}}
    "mongoose": "^5.12.5",
    {{/if_eq}}
    {{#if_eq database "mysql"}}
    "mysql": "^2.18.1",
    {{/if_eq}}
    {{#if_xor axios extraction "extraction"}}
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "module-alias": "^2.2.2",
    {{/if_xor}}
    {{#if_xor jsdom extraction "extraction"}}
    "jsdom": "^16.5.3",
    {{/if_xor}}
    {{#if_xor cookieparser extraction "extraction"}}
    "cookie-parser": "^1.4.5",
    {{/if_xor}}
    "express-import-routes": "^0.0.2-b6"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.2",
    "nodemon": "^2.0.7",
    {{#ts}}
    {{#if_xor cookieparser extraction "extraction"}}
    "@types/cookie-parser": "^1.4.2",
    {{/if_xor}}
    {{#if_xor jsdom extraction "extraction"}}
    "@types/jsdom": "^16.2.10",
    {{/if_xor}}
    {{#if_xor axios extraction "extraction"}}
    "@types/axios": "^0.14.0",
    {{/if_xor}}
    {{#if_eq database "mysql"}}
    "@types/mysql": "^2.15.18",
    {{/if_eq}}
    {{#if_eq database "mongoose"}}
    "@types/mongoose": "^5.10.5",
    {{/if_eq}}
    {{#morgan}}
    "@types/morgan": "^1.9.2",
    {{/morgan}}
    {{#helmet}}
    "@types/helmet": "^4.0.0",
    {{/helmet}}
    {{#if_xor cors extraction "extraction"}}
    "@types/cors": "^2.8.10",
    {{/if_xor}}
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0"
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.0.1",
    "@types/mocha": "^8.2.2",
    "@types/debug": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
    {{/ts}}
  }
}
